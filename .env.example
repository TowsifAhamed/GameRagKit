# Provider defaults
PROVIDER=openai
API_KEY=
ENDPOINT=

# Local engines
LOCAL_ENGINE=ollama
OLLAMA_HOST=http://host.docker.internal:11434
LOCAL_CHAT_MODEL=llama3.2:3b-instruct-q4_K_M
LOCAL_EMBED_MODEL=nomic-embed-text

# Cloud overrides
CLOUD_CHAT_MODEL=gpt-4o-mini
CLOUD_EMBED_MODEL=text-embedding-3-small

# Vector stores
DB=pgvector
CONNECTION_STRING=Host=postgres;Port=5432;Username=postgres;Password=postgres;Database=gamerag
QDRANT_ENDPOINT=http://qdrant:6333
QDRANT_COLLECTION=gamerag

# HTTP runtime
CORS_ORIGINS=
